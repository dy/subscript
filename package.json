{
  "name": "subscript",
  "version": "10.2.0",
  "description": "Modular expression parser & evaluator",
  "main": "subscript.js",
  "module": "subscript.js",
  "browser": "subscript.js",
  "types": "./subscript.d.ts",
  "exports": {
    ".": "./subscript.js",
    "./parse": "./parse.js",
    "./justin": "./justin.js",
    "./jessie": "./jessie.js",
    "./feature/*": "./feature/*",
    "./util/*": "./util/*"
  },
  "type": "module",
  "files": [
    "feature",
    "util",
    "subscript.js",
    "subscript.min.js",
    "subscript.d.ts",
    "parse.js",
    "justin.js",
    "justin.min.js",
    "jessie.js",
    "jessie.min.js"
  ],
  "scripts": {
    "build": "esbuild subscript.js --bundle --minify --format=esm --outfile=subscript.min.js && esbuild justin.js --bundle --minify --format=esm --outfile=justin.min.js && esbuild jessie.js --bundle --minify --format=esm --outfile=jessie.min.js",
    "prepublishOnly": "npm run build && npm run test:e2e",
    "test": "node --import ./test/https-loader.js test/test.js",
    "test:e2e": "npx playwright test test/index.spec.js",
    "check-types": "tsc --noEmit subscript.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dy/subscript.git"
  },
  "keywords": [
    "parser",
    "expression",
    "language",
    "dsl",
    "syntax",
    "modular",
    "extensible",
    "pratt",
    "evaluator",
    "compiler",
    "ast",
    "justin",
    "jessie",
    "jsep",
    "jexl",
    "sandboxed",
    "safe-eval",
    "formula",
    "template"
  ],
  "author": "Dmitry Iv.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dy/subscript/issues"
  },
  "homepage": "https://github.com/dy/subscript#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "esbuild": "^0.27.2",
    "terser": "^5.44.1",
    "tst": "^9.2.1"
  }
}
